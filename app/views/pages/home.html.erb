

  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3>Our favorite artists</h3>
      </div>
      <div class="panel-body">
<% @artists.each do |artist|%>
          <div class="row">
             <div class="col-md-7">
<h4><%= link_to artist.name, artist_path(artist.id) %></h4>
            </div>
<img src="<%= artist.image %>"/>
        </div>
        <br />
        <br />
<% end %>
      </div>
    </div>
</div>
